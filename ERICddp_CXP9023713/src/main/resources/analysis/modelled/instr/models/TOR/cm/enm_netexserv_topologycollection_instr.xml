<?xml version="1.0" encoding="UTF-8"?>
<modelledinstr namespace="^(\S+)-Instrumentation@e2e_\S+">
  <services>
    <service name="networkexplorer"/>
  </services>

  <table name="enm_netexserv_topologycollection_instr">
    <keycol name="serverid" reftable="servers"/>
  </table>

  <metricgroups>
    <metricgroup name="^%1%-com.ericsson.oss.services.topologyCollectionsService.service.instrumentation.topologyCollectionsService:type=InstrumentationBean$">
      <metric source="autoGeneratedCollectionsCount" target="autoGeneratedCollectionsCount" delta="true" filteridle="true"/>
      <metric source="collectionBatchesTotalTime" target="collectionBatchesTotalTime" delta="true" filteridle="true"/>
      <metric source="collectionDatabaseTotalTime" target="collectionDatabaseTotalTime" delta="true" filteridle="true"/>
      <metric source="collectionTotalTime" target="collectionTotalTime" delta="true" filteridle="true"/>
      <metric source="collectionWithContentsDatabaseTotalTime" target="collectionWithContentsDatabaseTotalTime" delta="true" filteridle="true"/>
      <metric source="collectionWithContentsTotalTime" target="collectionWithContentsTotalTime" delta="true" filteridle="true"/>
      <metric source="createCollectionDatabaseAverageSize" target="createCollectionDatabaseAverageSize" delta="true" filteridle="true"/>
      <metric source="createCollectionDatabaseLargestSize" target="createCollectionDatabaseLargestSize" delta="true" filteridle="true"/>
      <metric source="createCollectionDatabaseMedianSize" target="createCollectionDatabaseMedianSize" delta="true" filteridle="true"/>
      <metric source="createCollectionDatabaseTotalObjectCount" target="createCollectionDatabaseTotalObjectCount" delta="true" filteridle="true"/>
      <metric source="createCollectionDatabaseTotalTime" target="createCollectionDatabaseTotalTime" delta="true" filteridle="true"/>
      <metric source="createCollectionTotalTime" target="createCollectionTotalTime" delta="true" filteridle="true"/>
      <metric source="createSavedSearchDatabaseTotalTime" target="createSavedSearchDatabaseTotalTime" delta="true" filteridle="true"/>
      <metric source="createSavedSearchTotalTime" target="createSavedSearchTotalTime" delta="true" filteridle="true"/>
      <metric source="deleteCollectionDatabaseTotalTime" target="deleteCollectionDatabaseTotalTime" delta="true" filteridle="true"/>
      <metric source="deleteCollectionTotalTime" target="deleteCollectionTotalTime" delta="true" filteridle="true"/>
      <metric source="deleteSavedSearchDatabaseTotalTime" target="deleteSavedSearchDatabaseTotalTime" delta="true" filteridle="true"/>
      <metric source="deleteSavedSearchTotalTime" target="deleteSavedSearchTotalTime" delta="true" filteridle="true"/>
      <metric source="privateCollectionsCount" target="privateCollectionsCount" delta="true" filteridle="true"/>
      <metric source="privateSavedSearchesCount" target="privateSavedSearchesCount" delta="true" filteridle="true"/>
      <metric source="publicCollectionsCount" target="publicCollectionsCount" delta="true" filteridle="true"/>
      <metric source="publicSavedSearchesCount" target="publicSavedSearchesCount" delta="true" filteridle="true"/>
      <metric source="savedSearchesDatabaseTotalTime" target="savedSearchesDatabaseTotalTime" delta="true" filteridle="true"/>
      <metric source="savedSearchesTotalTime" target="savedSearchesTotalTime" delta="true" filteridle="true"/>
      <metric source="updateCollectionDatabaseTotalTime" target="updateCollectionDatabaseTotalTime" delta="true" filteridle="true"/>
      <metric source="updateCollectionTotalTime" target="updateCollectionTotalTime" delta="true" filteridle="true"/>
      <metric source="labelsCount" target="labelsCount" delta="true" filteridle="true"/>
    </metricgroup>
  </metricgroups>

</modelledinstr>
