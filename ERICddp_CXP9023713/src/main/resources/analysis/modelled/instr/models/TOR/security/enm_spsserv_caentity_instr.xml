<?xml version="1.0" encoding="UTF-8"?>
<modelledinstr namespace="^(\S+)-Instrumentation@e2e_\S+">
  <services>
    <service name="sps"/>
  </services>

  <table name="enm_spsserv_caentity_instr">
    <keycol name="serverid" reftable="servers"/>
  </table>

  <metricgroups>
    <metricgroup name="^%1%-com.ericsson.oss.itpf.security.pki.manager.instrumentation.core.metrics.pki-manager:type=CAEntityCertificateManagementInstrumentationBean$">
      <metric source="generateExecutionTimeTotalMillis" target="generateExecutionTimeTotalMillis" delta="true"/>
      <metric source="generateMethodFailures" target="generateMethodFailures" delta="true"/>
      <metric source="generateMethodInvocations" target="generateMethodInvocations" delta="true"/>
      <metric source="rekeyExecutionTimeTotalMillis" target="rekeyExecutionTimeTotalMillis" delta="true"/>
      <metric source="rekeyMethodFailures" target="rekeyMethodFailures" delta="true"/>
      <metric source="rekeyMethodInvocations" target="rekeyMethodInvocations" delta="true"/>
      <metric source="renewExecutionTimeTotalMillis" target="renewExecutionTimeTotalMillis" delta="true"/>
      <metric source="renewMethodFailures" target="renewMethodFailures" delta="true"/>
      <metric source="renewMethodInvocations" target="renewMethodInvocations" delta="true"/>
      <metric source="deleteExecutionTimeTotalMillis" target="deleteExecutionTimeTotalMillis" delta="true"/>
      <metric source="deleteMethodFailures" target="deleteMethodFailures" delta="true"/>
      <metric source="deleteMethodInvocations" target="deleteMethodInvocations" delta="true"/>
    </metricgroup>
  </metricgroups>

</modelledinstr>
