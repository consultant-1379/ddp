<?xml version="1.0" encoding="UTF-8"?>
<modelledinstr namespace="^(\S+)-Instrumentation@e2e_\S+">
  <services>
    <service name="sps"/>
  </services>

  <table name="enm_spsserv_entity_instr">
    <keycol name="serverid" reftable="servers"/>
  </table>

  <metricgroups>
    <metricgroup name="^%1%-com.ericsson.oss.itpf.security.pki.manager.instrumentation.core.metrics.pki-manager:type=EntityManagementInstrumentationBean$">
      <metric source="createExecutionTimeTotalMillis" target="createExecutionTimeTotalMillis" delta="true"/>
      <metric source="createMethodFailures" target="createMethodFailures" delta="true"/>
      <metric source="createMethodInvocations" target="createMethodInvocations" delta="true"/>
      <metric source="deleteExecutionTimeTotalMillis" target="deleteExecutionTimeTotalMillis" delta="true"/>
      <metric source="deleteMethodFailures" target="deleteMethodFailures" delta="true"/>
      <metric source="deleteMethodInvocations" target="deleteMethodInvocations" delta="true"/>
      <metric source="readExecutionTimeTotalMillis" target="getExecutionTimeTotalMillis" delta="true"/>
      <metric source="readMethodFailures" target="getMethodFailures" delta="true"/>
      <metric source="readMethodInvocations" target="getMethodInvocations" delta="true"/>
      <metric source="updateExecutionTimeTotalMillis" target="updateExecutionTimeTotalMillis" delta="true"/>
      <metric source="updateMethodFailures" target="updateMethodFailures" delta="true"/>
      <metric source="updateMethodInvocations" target="updateMethodInvocations" delta="true"/>
    </metricgroup>
  </metricgroups>

</modelledinstr>
