          <metric source="receivedCliMessageInvokeAutoCommit" target="receivedCliMessageInvokeAutoCommit" delta="true" filteridle="true"/>
          <metric source="failedCliMessageInvokeAutoCommit" target="failedCliMessageInvokeAutoCommit" delta="true" filteridle="true"/>
          <metric source="receivedCliMessageAbort" target="receivedCliMessageAbort" delta="true" filteridle="true"/>
          <metric source="failedCliMessageAbort" target="failedCliMessageAbort" delta="true" filteridle="true"/>
      </metricgroup>

      <metricgroup name="^%1%-com.ericsson.oss.services.ncm.agent.instrumentation.ncm-agent:type=NcmAgentR6kCliMessageMbean$">
