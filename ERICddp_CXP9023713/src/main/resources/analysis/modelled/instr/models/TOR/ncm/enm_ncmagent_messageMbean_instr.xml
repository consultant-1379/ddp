          <metric source="failedR6kCliMessageInvokeAutoCommitDuration" target="failedR6KCliMessageInvokeAutoCommitDuration"/>
          <metric source="successR6kCliMessageAbortDuration" target="successR6KCliMessageAbortDuration"/>
          <metric source="failedR6kCliMessageAbortDuration" target="failedR6KCliMessageAbortDuration"/>
