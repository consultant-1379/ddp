<?xml version="1.0" encoding="UTF-8"?>
<modelledinstr namespace="^(\S+)-Instrumentation@e2e_\S+">
  <services>
      <service name="shmcoreservice"/>
      <service name="consshm"/>
  </services>

  <table name="shm_nejob_instr">
    <keycol name="serverid" reftable="servers"/>
  </table>

  <metricgroups>
    <metricgroup name="^%1%-com.ericsson.oss.services.shm.es.instrumentation.impl.cppinventorysynchservice:type=NeJobInstrumentaionBean$">
      <metric source="cppBackupNeJobs" target="cppBackupNeJobs"/>
      <metric source="cppDeleteBackupNeJobs" target="cppDeleteBackupNeJobs"/>
      <metric source="cppDeleteUpgradePackageNeJobs" target="cppDeleteUpgradePackageNeJobs"/>
      <metric source="cppLicenseNeJobs" target="cppLicenseNeJobs"/>
      <metric source="cppRestoreNeJobs" target="cppRestoreNeJobs"/>
      <metric source="cppUpgradeNeJobs" target="cppUpgradeNeJobs"/>
      <metric source="ecimBackupNeJobs" target="ecimBackupNeJobs"/>
      <metric source="ecimDeleteBackupNeJobs" target="ecimDeleteBackupNeJobs"/>
      <metric source="ecimDeleteUpgradePackageNeJobs" target="ecimDeleteUpgradePackageNeJobs"/>
      <metric source="ecimLicenseNeJobs" target="ecimLicenseNeJobs"/>
      <metric source="ecimRestoreNeJobs" target="ecimRestoreNeJobs"/>
      <metric source="ecimUpgradeNeJobs" target="ecimUpgradeNeJobs"/>
      <metric source="ecimNodeHealthCheckNeJobscount" target="ecimNodeHealthCheckNeJobscount"/>
      <metric source="AXEUpgradeNeJobs" target="AXEUpgradeNeJobs"/>
      <metric source="axeLicenseNeJobs" target="axeLicenseNeJobs"/>
      <metric source="AXEBackupNeJobs" target="AXEBackupNeJobs"/>
      <metric source="AXEDeleteBackupNeJobs" target="AXEDeleteBackupNeJobs"/>
      <metric source="ecimLicenseRefreshNeJobscount" target="ecimLicenseRefreshNeJobscount"/>
      <metric source="vranUpgradeNeJobscount" target="vranUpgradeNeJobscount"/>
    </metricgroup>
  </metricgroups>

</modelledinstr>
