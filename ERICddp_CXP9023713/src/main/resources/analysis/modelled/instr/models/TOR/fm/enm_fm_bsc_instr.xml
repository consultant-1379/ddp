<?xml version="1.0" encoding="UTF-8"?>
<modelledinstr namespace="^(\S+)-Instrumentation@e2e_\S+">
  <services>
    <service name="msapgfm"/>
  </services>

  <table name="enm_fm_bsc_instr">
    <keycol name="serverid" reftable="servers"/>
  </table>

  <metricgroups>
    <metricgroup name="^%1%-com.ericsson.oss.mediation.fm.axe.engine.instrumentation.axe-fm-mediation-engine:type=AxeEngineStatisticsExt$">
      <metric source="alarmsProcessingFailure" target="alarmsProcessingFailure" delta="true"/>
      <metric source="alarmsProcessingSuccess" target="alarmsProcessingSuccess" delta="true"/>
      <metric source="axeAlarmsDiscarded" target="axeAlarmsDiscarded" delta="true"/>
      <metric source="axeAlarmsReceived" target="axeAlarmsReceived" delta="true"/>
      <metric source="heartBeatPingsReceived" target="heartBeatPingsReceived" delta="true"/>
      <metric source="processedAlarmsForwarded" target="processedAlarmsForwarded" delta="true"/>
      <metric source="processedAlarmsForwardedFailure" target="processedAlarmsForwardedFailure" delta="true"/>
      <metric source="processedSyncAlarmsForwarded" target="processedSyncAlarmsForwarded" delta="true"/>
      <metric source="processedSyncAlarmsForwardedFailures" target="processedSyncAlarmsForwardedFailures" delta="true"/>
      <metric source="spontAlarmsReceived" target="spontAlarmsReceived" delta="true"/>
      <metric source="syncAlarmsReceived" target="syncAlarmsReceived" delta="true"/>
      <metric source="unKnownAlarmTypesReceived" target="unKnownAlarmTypesReceived" delta="true"/>
      <metric source="numOfHBFailureNodes" target="numOfHBFailureNodes"/>
      <metric source="numOfSupervisedNodes" target="numOfSupervisedNodes"/>
    </metricgroup>
  </metricgroups>

</modelledinstr>
