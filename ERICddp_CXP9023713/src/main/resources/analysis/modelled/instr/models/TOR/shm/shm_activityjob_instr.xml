      <metric source="cppRestoreInstallCVs" target="cppRestoreInstallCVs"/>
      <metric source="cppRestoreRestores" target="cppRestoreRestores"/>
      <metric source="cppRestoreVerifyCVs" target="cppRestoreVerifyCVs"/>
      <metric source="cppUpgradeConfirms" target="cppUpgradeConfirms"/>
      <metric source="cppUpgradeInstalls" target="cppUpgradeInstalls"/>
